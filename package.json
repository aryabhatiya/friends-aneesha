{
  "name": "friends-aneesha",
  "version": "0.0.0-semanticly-release",
  "description": "New Module bootstrap",
  "main": "src/index.js",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "codecov.io": "0.1.6",
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "ghooks": "1.0.3",
    "istanbul": "0.4.2",
    "mocha": "2.3.4",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit_strict": "npm run test:single && npm run check-coverage",
      "pre-commit": "npm run test:single"
    }
  },
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chowdhuryashraf/friends-aneesha.git"
  },
  "keywords": [
    "bootstart",
    "module"
  ],
  "author": "Chowdhury Ashraf",
  "license": "MIT"
}
